<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.liy.Vivero.Model">
  <class name="Usuario" table="Usuario">
      
    <id name="id"
	    column="id"
	    type="java.lang.Integer"
	    unsaved-value="null" >
	    <generator class="native">
	    </generator>
    </id>
    
    <property name="nombre" column="nombre" type="java.lang.String"/>
    <property name="apellido" column="apellido" type="java.lang.String"/>
    <property name="correo" column="correo" type="java.lang.String"/>
    <property name="usuario" column="usuario" type="java.lang.String"/>
    <property name="contrasenia" column="contrasenia" type="java.lang.String"/>
    

  </class>
</hibernate-mapping>
