<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.liy.Vivero.View.HistorialController">
   <children>
      <ImageView fitHeight="450.0" fitWidth="562.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/29retopiensaverde1.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="idProducto" layoutX="21.0" layoutY="79.0" promptText="ID producto" />
      <Button layoutX="317.0" layoutY="79.0" mnemonicParsing="false" onAction="#actualizarClick" text="Acceder" />
      <TableView fx:id="tabla" layoutX="21.0" layoutY="116.0" prefHeight="193.0" prefWidth="355.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="42.0" text="ID" />
          <TableColumn fx:id="fotografia" prefWidth="169.0" text="Foto" />
            <TableColumn fx:id="fecha" prefWidth="135.0" text="Fecha" />
        </columns>
      </TableView>
      <Button layoutX="33.0" layoutY="340.0" mnemonicParsing="false" onAction="#cerrarClick" text="Regresar" />
      <Button layoutX="300.0" layoutY="344.0" mnemonicParsing="false" onAction="#agregarClick" text="Agregar" />
   </children>
</AnchorPane>
